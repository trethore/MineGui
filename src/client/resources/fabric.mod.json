{
  "schemaVersion": 1,
  "id": "minegui",
  "version": "${version}",
  "name": "MineGui",
  "description": "MineGui is a lightweight wrapper around `imgui-java`, designed to simplify the creation of modern GUIs in Minecraft mods built with Fabric.",
  "authors": [
    "Tytoo"
  ],
  "contact": {
    "sources": "https://github.com/trethore/MineGui"
  },
  "license": "CC0-1.0",
  "icon": "assets/minegui/icon.png",
  "environment": "*",
  "entrypoints": {},
  "mixins": [
    {
      "config": "minegui.client.mixins.json",
      "environment": "client"
    }
  ],
  "depends": {
    "fabricloader": ">=0.17.3",
    "minecraft": "~1.21.4",
    "java": ">=21",
    "fabric-api": ">=0.119.0+1.21.4"
  },
  "suggests": {
    "another-mod": "*"
  }
}